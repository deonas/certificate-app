generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int    @id @default(autoincrement())
  name  String
  email String @unique
}

model Certificate {
  id            Int      @id @default(autoincrement())
  certificateId String   @unique
  name          String
  issuer        String
  date          DateTime
  url           String
}
